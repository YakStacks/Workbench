{
  "name": "workbench",
  "version": "0.1.0",
  "main": "main.cjs",
  "type": "module",
  "scripts": {
    "dev": "concurrently -k \"vite\" \"wait-on http://localhost:5173 && npm run build:electron && electron .\"",
    "build": "vite build && npm run build:electron",
    "build:electron": "tsc main.ts preload.ts --outDir . --esModuleInterop --module commonjs --skipLibCheck && node -e \"require('fs').renameSync('main.js','main.cjs');require('fs').renameSync('preload.js','preload.cjs')\"",
    "start": "electron ."
  },
  "dependencies": {
    "axios": "^1.13.4",
    "electron": "^29.0.0",
    "electron-store": "^8.1.0",
    "node-fetch": "^3.3.2",
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "devDependencies": {
    "@types/react": "^19.2.10",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^4.0.0",
    "concurrently": "^9.2.1",
    "typescript": "^5.0.0",
    "vite": "^5.0.0",
    "wait-on": "^9.0.3"
  }
}
